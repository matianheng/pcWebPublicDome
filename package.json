{
  "name": "archetype-backend-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.8.0"
  },
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "compress-svg": "ts-node ./build-script/compressSvg.ts",
    "move-sourcemap": "ts-node ./build-script/moveSourceMapFile.ts",
    "compile-dist-scss-to-css": "ts-node ./build-script/buildPublicPrintCss.ts",
    "test-vitest": "vitest run -c ./vite.config.ts",
    "vitest-ui": "vitest --ui",
    "dev": "vite",
    "build": "rimraf -rf ./dist && rimraf -rf ./js-maps && npm run compress-svg && vue-tsc && vite build && npm run move-sourcemap && npm run compile-dist-scss-to-css",
    "package_analyzer": "rimraf -rf ./dist && vue-tsc && vite build --mode analyzer",
    "rm_vite_cache": "rimraf -rf ./node_modules/.vite",
    "preview": "vite preview",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md,vue}\"",
    "lint:style": "stylelint \"src/**/*.(vue|less|scss|css)\" --fix",
    "eslint-fix": "eslint --ext .ts,.js,.vue src/ --cache --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test": "jest",
    "test-c": "jest --coverage",
    "git-commit": "git-cz",
    "release": "standard-version",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@floating-ui/dom": "^1.6.1",
    "@tinymce/tinymce-vue": "5.1.1",
    "@ungap/global-this": "^0.4.4",
    "@vue/shared": "^3.4.5",
    "@vueuse/core": "^10.7.0",
    "animate.css": "^4.1.1",
    "axios": "^1.6.1",
    "big.js": "^6.2.1",
    "core-js": "^3.34.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.5.3",
    "globalthis": "^1.0.3",
    "js-cookie": "^3.0.5",
    "lodash-unified": "^1.0.3",
    "mitt": "^3.0.1",
    "pdfjs-dist": "2.12",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "regenerator-runtime": "^0.13.11",
    "tinymce": "^6.8.1",
    "ts-md5": "^1.3.1",
    "typescript-cookie": "^1.0.6",
    "vue": "^3.4.5",
    "vue-draggable-plus": "^0.3.3",
    "vue-i18n": "^9.6.5",
    "vue-request": "^1.2.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@types/big.js": "^6.2.2",
    "@types/jest": "^29.5.8",
    "@types/js-cookie": "^3.0.6",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.18.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/ui": "^1.2.2",
    "@vue/runtime-core": "^3.3.8",
    "@vue/test-utils": "^2.4.4",
    "commitlint": "^17.8.1",
    "consola": "^2.15.3",
    "cssnano": "^5.1.15",
    "edge-runtime": "^2.5.8",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.18.1",
    "git-cz": "^4.9.0",
    "glob": "^10.3.10",
    "happy-dom": "^13.3.8",
    "htmlnano": "^2.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^13.3.0",
    "mockjs": "^1.1.0",
    "msw": "^2.1.7",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.8.8",
    "query-string": "^8.2.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.11.0",
    "sass": "^1.69.5",
    "standard-version": "^9.5.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.0.0",
    "svgo": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock-dev-server": "^1.4.3",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "7.0.5",
    "vitest": "^1.1.3",
    "vue-component-type-helpers": "^1.8.22",
    "vue-tsc": "^1.8.27"
  },
  "browserslist": [
    "defaults",
    "Android >= 4.4",
    "iOS >= 12.0",
    "ie >= 11",
    "Firefox > 69",
    "chrome >= 69"
  ],
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "src/**/*.{less,css,scss,vue}": [
      "stylelint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
